#compdef generate_completions.py

# AUTOMATICALLY GENERATED by `shtab`


_shtab_generate_completions_py_commands() {
  local _commands=(
    "refactor:"
    "report:"
    "validate:"
  )
  _describe 'generate_completions.py commands' _commands
}

_shtab_generate_completions_py_options=(
  "(- : *)"{-h,--help}"[show this help message and exit]"
  "--path[Path to scan or refactor (default\: current directory)]:path:"
  "--constants-file[File name for storing global constants]:constants_file:"
  "--min-length[Minimum length for string literals (default\: 4)]:min_length:"
  "--min-count[Minimum occurrences for a constant to be considered (default\: 2)]:min_count:"
  "--no-local-scope[Force all constants to be global (ignore local scope)]"
  "*--ignore-call[Ignore specific function calls during scanning]:ignore_call:"
  "*--exclude[Directories to exclude]:exclude:"
  "*--ignore-num[Numbers to ignore]:ignore_num:"
  "*--include-num[Numbers to include (un-ignore)]:include_num:"
  "*--ignore-str[Strings to ignore]:ignore_str:"
  "--no-numbers[Skip number scanning]"
  "--no-ints[Skip integer scanning]"
  "--no-floats[Skip float scanning]"
  "--no-bytes[Skip bytes scanning]"
  "--naming[Strategy for naming generated constants]:naming:(derived generic)"
  "--extra-constants[Additional Python files to scan for constants]:extra_constants:"
  "--apply[Apply detected changes to files (direct mode only)]"
)

# guard to ensure default positional specs are added only once per session
_shtab_generate_completions_py_defaults_added=0

_shtab_generate_completions_py_refactor_options=(
  "(- : *)"{-h,--help}"[show this help message and exit]"
  "--apply[Apply detected changes to files]"
)

# guard to ensure default positional specs are added only once per session
_shtab_generate_completions_py_refactor_defaults_added=0

_shtab_generate_completions_py_report_options=(
  "(- : *)"{-h,--help}"[show this help message and exit]"
)

# guard to ensure default positional specs are added only once per session
_shtab_generate_completions_py_report_defaults_added=0

_shtab_generate_completions_py_validate_options=(
  "(- : *)"{-h,--help}"[show this help message and exit]"
  "--report-file[Path to the report file to validate]:report_file:"
)

# guard to ensure default positional specs are added only once per session
_shtab_generate_completions_py_validate_defaults_added=0


_shtab_generate_completions_py() {
  local context state line curcontext="$curcontext" one_or_more='(*)' remainder='(-)*' default='*::: :->generate_completions.py'

  # Add default positional/remainder specs only if none exist, and only once per session
  if (( ! _shtab_generate_completions_py_defaults_added )); then
    if (( ${_shtab_generate_completions_py_options[(I)${(q)one_or_more}*]} +          ${_shtab_generate_completions_py_options[(I)${(q)remainder}*]} +          ${_shtab_generate_completions_py_options[(I)${(q)default}]} == 0 )); then
      _shtab_generate_completions_py_options+=(': :_shtab_generate_completions_py_commands' '*::: :->generate_completions.py')
    fi
    _shtab_generate_completions_py_defaults_added=1
  fi
  _arguments -C -s $_shtab_generate_completions_py_options

  case $state in
    generate_completions.py)
      words=($line[1] "${words[@]}")
      (( CURRENT += 1 ))
      curcontext="${curcontext%:*:*}:_shtab_generate_completions_py-$line[1]:"
      case $line[1] in
        refactor) _arguments -C -s $_shtab_generate_completions_py_refactor_options ;;
        report) _arguments -C -s $_shtab_generate_completions_py_report_options ;;
        validate) _arguments -C -s $_shtab_generate_completions_py_validate_options ;;
      esac
  esac
}



typeset -A opt_args

if [[ $zsh_eval_context[-1] == eval ]]; then
  # eval/source/. command, register function for later
  compdef _shtab_generate_completions_py -N generate_completions.py
else
  # autoload from fpath, call function directly
  _shtab_generate_completions_py "$@"
fi
